#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/data/data/com.termux/files/home/dev/node_modules/.pnpm/vuepress@2.0.0-rc.21_@vuepress+bundler-vite@2.0.0-rc.21_@types+node@22.14.1_sass-embedded@1.86.3__vue@3.5.13/node_modules/vuepress/bin/node_modules:/data/data/com.termux/files/home/dev/node_modules/.pnpm/vuepress@2.0.0-rc.21_@vuepress+bundler-vite@2.0.0-rc.21_@types+node@22.14.1_sass-embedded@1.86.3__vue@3.5.13/node_modules/vuepress/node_modules:/data/data/com.termux/files/home/dev/node_modules/.pnpm/vuepress@2.0.0-rc.21_@vuepress+bundler-vite@2.0.0-rc.21_@types+node@22.14.1_sass-embedded@1.86.3__vue@3.5.13/node_modules:/data/data/com.termux/files/home/dev/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/data/data/com.termux/files/home/dev/node_modules/.pnpm/vuepress@2.0.0-rc.21_@vuepress+bundler-vite@2.0.0-rc.21_@types+node@22.14.1_sass-embedded@1.86.3__vue@3.5.13/node_modules/vuepress/bin/node_modules:/data/data/com.termux/files/home/dev/node_modules/.pnpm/vuepress@2.0.0-rc.21_@vuepress+bundler-vite@2.0.0-rc.21_@types+node@22.14.1_sass-embedded@1.86.3__vue@3.5.13/node_modules/vuepress/node_modules:/data/data/com.termux/files/home/dev/node_modules/.pnpm/vuepress@2.0.0-rc.21_@vuepress+bundler-vite@2.0.0-rc.21_@types+node@22.14.1_sass-embedded@1.86.3__vue@3.5.13/node_modules:/data/data/com.termux/files/home/dev/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vuepress-vite.js" "$@"
else
  exec node  "$basedir/../../bin/vuepress-vite.js" "$@"
fi
