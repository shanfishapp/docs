#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/data/data/com.termux/files/home/dev/node_modules/.pnpm/vite@6.2.6_@types+node@22.14.1_sass-embedded@1.86.3/node_modules/vite/bin/node_modules:/data/data/com.termux/files/home/dev/node_modules/.pnpm/vite@6.2.6_@types+node@22.14.1_sass-embedded@1.86.3/node_modules/vite/node_modules:/data/data/com.termux/files/home/dev/node_modules/.pnpm/vite@6.2.6_@types+node@22.14.1_sass-embedded@1.86.3/node_modules:/data/data/com.termux/files/home/dev/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/data/data/com.termux/files/home/dev/node_modules/.pnpm/vite@6.2.6_@types+node@22.14.1_sass-embedded@1.86.3/node_modules/vite/bin/node_modules:/data/data/com.termux/files/home/dev/node_modules/.pnpm/vite@6.2.6_@types+node@22.14.1_sass-embedded@1.86.3/node_modules/vite/node_modules:/data/data/com.termux/files/home/dev/node_modules/.pnpm/vite@6.2.6_@types+node@22.14.1_sass-embedded@1.86.3/node_modules:/data/data/com.termux/files/home/dev/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vite.js" "$@"
else
  exec node  "$basedir/../../bin/vite.js" "$@"
fi
